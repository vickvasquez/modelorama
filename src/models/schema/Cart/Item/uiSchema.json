{
  "title": "New gift!",
  "qty": {
    "ui:title": "Quantity",
    "ui:widget": "updown",
    "ui:autofocus": true
  },
  "cart_id": {
    "ui:xdisabled": true,
    "ui:template": [
      ["small", "total: {items.CartItem.qty!0}"],
      ["span", "{id} - {@val:items.*.name}"]
    ]
  },
  "product_id": {
    "ui:template": [
      ["small", "${price}"],
      ["span", "{name}"]
    ]
  }
}
